{
  "name": "opensearch-sink",
  "config": {
    "connector.class": "io.confluent.connect.opensearch.OpensearchSinkConnector",
    "tasks.max": "1",
    "topics.regex": "shopserver\\.public\\.products",
    "connection.url": "http://opensearch:9200",
    "type.name": "_doc",
    "behavior.on.null.values": "IGNORE",
    "key.ignore": "true",
    "schema.ignore": "true",
    "write.method": "upsert",
    "batch.size": "2000",
    "max.in.flight.requests": "5",
    "linger.ms": "50",
    "max.buffered.records": "20000",
    "batch.size.bytes": "5242880",
    "flush.timeout.ms": "60000",
    "max.retries": "10",
    "retry.backoff.ms": "5000",
    "drop.invalid.message": "true",
    "name": "opensearch-sink",
    "transforms": "unwrap",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "true",
    "transforms.unwrap.delete.handling.mode": "drop",
    "behavior.on.malformed.documents": "IGNORE",
    "auto.create.indices.at.start": "true",
    "connection.username": "admin",
    "connection.password": "admin"
  }
}